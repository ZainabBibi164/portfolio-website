rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /contacts/{document=**} {
      allow write: if request.resource.data.securityKey == "kock tols fqex chiw";
      allow read: if false; // Restrict reads for security
    }
    match /github_cache/{document=**} {
      allow read, write: if request.auth == null; // For Projects.jsx caching
    }
  }
}